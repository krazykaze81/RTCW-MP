// OSP crosshair definitions

gfx/2d/crosshaira_OSP
{
	nopicmip
	nocompress
	{
		map gfx/2d/crosshaira.tga
		blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		rgbGen vertex
	}
}

